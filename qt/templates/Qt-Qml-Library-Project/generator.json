{
    "ignoreFilePattern": [".*swp", "pro.user$", ".DS_Store", "qmlc$", "build-" , "jsc$"],
    "rules": [
        {
            "pattern" : ".*",
            "path": [
                {
                    "find": "MYPROJECT",
                    "replace": "%{ProjectName}"
                }            
            ],
            "content": [
                {
                    "find": "MYPROJECT",
                    "replace": "%{ProjectName}"
                }
            ]
        },
        {
            "pattern": "MYPROJECT.pro$",
            "openAsProject": true
        },
        {
            "pattern": ".*",
            "content": [
                {
                    "find": "\\\\",
                    "replace": "\\\\"
                }
            ]
        }        
    ]
}
