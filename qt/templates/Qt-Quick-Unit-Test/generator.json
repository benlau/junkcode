{
    "ignoreFilePattern": [
        "\\.swp$",
        "\\.DS_Store$"
    ],
    "rules": [
        {
            "pattern": ".*",
            "path": [
                {
                    "find": "PROJECT",
                    "replace" : "%{ProjectName}"
                }

            ],
            "content": [
                {
                    "find": "\\",
                    "replace": "\\"
                },
                {
                    "find": "PROJECT",
                    "replace" : "%{ProjectName}"
                }
            ]
        },
        {
            "pattern": "\\.pro$",
            "openAsProject": true
        }
    ]
}
