{
    "version": 1,
    "kind": "project",
    "id": "EF.Project.adffa1",
    "category": "F.Application",
    "trDescription": "A custom wizard generated by qtcwizard from E-Fever Limited",
    "trDisplayName": "Example Project",
    "trDisplayCategory": "Application",
    "icon": "",
    "enabled": "%{JS: [ %{Plugins} ].indexOf('QmakeProjectManager') >= 0}",
    "options": [
        {
            "key": "QtQuickVersion",
            "value": "%{JS: %{QtVersion}.qtQuickVersion}"
        },
        {
            "key": "ProFileName",
            "value": "%{JS: Util.fileName('%{ProjectDirectory}/%{ProjectName}', 'pro')}"
        }
    ],
    "pages": [
        {
            "trDisplayName": "Project Location",
            "trShortTitle": "Location",
            "typeId": "Project"
        },
        {
            "trDisplayName": "Project Management",
            "trShortTitle": "Summary",
            "typeId": "Summary"
        }
    ],
    "generators": [
        {
            "typeId": "File",
            "data": [
                {
                    "source": ".gitignore",
                    "target": ".gitignore"
                },
                {
                    "source": "main.cpp",
                    "target": "main.cpp"
                },
                {
                    "source": "main.qml",
                    "target": "main.qml"
                },
                {
                    "source": "Page1.qml",
                    "target": "Page1.qml"
                },
                {
                    "source": "Page1Form.ui.qml",
                    "target": "Page1Form.ui.qml"
                },
                {
                    "source": "project1.pro",
                    "target": "%{ProFileName}",
                    "openAsProject": true
                },
                {
                    "source": "qml.qrc",
                    "target": "qml.qrc"
                },
                {
                    "source": "qtquickcontrols2.conf",
                    "target": "qtquickcontrols2.conf"
                }
            ]
        }
    ]
}